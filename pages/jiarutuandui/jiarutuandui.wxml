
<view class="container">
  <view class="login-icon">
    <image class="login-img" src="/image/众人.png"></image>
  </view>
  <view class="login-from">
 
    <!--账号-->
    <view class="inputView">
      <label class="loginLab">团队编号</label>
      <input class="inputText" placeholder="请输入编号" bindinput="phoneInput" />
    </view>
    <view class="line"></view>
 
    <!--密码-->
    <view class="inputView">

      <label class="loginLab">密码</label>
      <input class="inputText" password="true" placeholder="请输入密码" bindinput="passwordInput" />
    </view>
 
    <!--按钮-->
    <view class="loginBtnView">
      <button class="loginBtn" type="primary" size="{{primarySize}}" loading="{{loading}}" plain="{{plain}}" disabled="{{disabled}}" bindtap="login">加入</button>
    </view>
  </view>
</view>